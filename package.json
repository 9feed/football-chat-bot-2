{
  "name": "football-chat-bot-2",
  "version": "1.0.0",
  "description": "Footbal chat bot build with NestJS framework",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './git/commitlint.js'"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./git/commitizen.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tormozz48/football-chat-bot-2.git"
  },
  "keywords": [
    "football",
    "chat",
    "bot",
    "telegram",
    "telegram",
    "bot",
    "nestjs",
    "nodejs",
    "typescript"
  ],
  "author": "andrey.kuznetsov48@yandex.ru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tormozz48/football-chat-bot-2/issues"
  },
  "homepage": "https://github.com/tormozz48/football-chat-bot-2#readme",
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@types/handlebars": "^4.1.0",
    "dotenv": "^8.2.0",
    "events-extra": "^1.0.4",
    "handlebars": "^4.5.2",
    "handlebars-helpers": "^0.10.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "read-dir-deep": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "socks5-https-client": "^1.2.1",
    "sqlite3": "^4.1.0",
    "telegraf": "^3.33.3",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.7.5",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
